  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^CustomGroup(.*)$": "<rootDir>/src/components/molecules/GroupCreatingModal/components/CustomGroup$1",
      "^DiagramWidgetEditForm(.*)$": "<rootDir>/src/components/organisms/DiagramWidgetEditForm$1",
      "^Table(.*)$": "<rootDir>/src/components/organisms/Table$1"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "(/tests/helpers.*|(\\.|/)(test|spec))\\.jsx?$"
    ]
