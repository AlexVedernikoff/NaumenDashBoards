  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^GroupModal(.*)$": "<rootDir>/src/components/organisms/GroupModal$1",
      "^WidgetFormPanel(.*)$": "<rootDir>/src/components/organisms/WidgetFormPanel$1",
      "^Table(.*)$": "<rootDir>/src/components/organisms/Table$1",
      "^TableWidgetForm(.*)$": "<rootDir>/src/components/organisms/TableWidgetForm$1",
      "^src(.*)$": "<rootDir>/src$1"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/tests/helpers.*\\.jsx?$"
    ]
  },
  "keywords": [
    "naumen",
    "embedded",
    "nordclan",
    "react"
  ],
  "license": "ISC",
  "name": "nau-chart",
  "repository": {
    "type": "git",
    "url": "git@gitsd.naumen.ru:nordclan/nordclan.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/config.js && npm run zip",
    "build-dev": "npm run clear && cross-env NODE_ENV=development webpack --config ./webpack/config.js && npm run zip",
    "build-license": "npm run clear && cross-env NODE_ENV=production LICENSE=use webpack --config ./webpack/config.js && npm run zip",
    "clear": "rm -rf ./dist/*",
    "clear-zip": "rm *.zip",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/config.js",
    "flow": "flow",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx ./src",
    "fix": "./node_modules/.bin/eslint --fix --ext .js,.jsx ./src",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "zip": "cd dist/ && bestzip ../%npm_package_name%-v%npm_package_version%.zip *",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "version": "1.1.3"
